        "@types/supertest": "^2.0.12",
        "@typescript-eslint/eslint-plugin": "^5.21.0",
        "@typescript-eslint/parser": "^5.21.0",
        "eslint": "^8.14.0",
        "eslint-config-prettier": "^8.5.0",
        "eslint-plugin-prettier": "^4.0.0",
        "faker": "^6.6.6",
        "jest": "^27.1.4",
        "memory-streams": "^0.1.3",
        "prettier": "^2.6.2",
        "rimraf": "^3.0.2",
        "supertest": "^6.2.3",
        "ts-jest": "^27.1.4",
        "ts-loader": "^9.2.9",
        "ts-node": "^10.7.0",
        "tsconfig-paths": "^3.14.1",
        "typescript": "^4.6.3"
