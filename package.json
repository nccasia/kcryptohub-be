        "class-validator": "^0.13.2",
        "compression": "^1.7.4",
        "cookie-parser": "^1.4.6",
        "dotenv": "^16.0.0",
        "ethers": "^5.6.4",
        "factory.ts": "^1.1.0",
        "ipfs-http-client": "^56.0.3",
        "moment": "^2.29.3",
        "passport-jwt": "^4.0.0",
        "pg": "^8.7.3",
        "reflect-metadata": "^0.1.13",
        "rxjs": "^7",
